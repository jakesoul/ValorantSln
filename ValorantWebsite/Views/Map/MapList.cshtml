@model MapsListViewModel

<h2>Valorant Maps</h2>
@foreach (var m in Model.Maps ?? Enumerable.Empty<Map>())
{
    <partial name="MapSummary" model="m" />
}

<div page-model='@Model.PagingInfo' page-action='MapList'
     page-classes-enabled='true' page-class='btn'
     page-class-normal='btn-outline-dark'
     page-class-selected='btn-primary'
     page-url-mapName='@Model.CurrentMapName!'
     class='btn-group pull-right m-1'>
</div>
