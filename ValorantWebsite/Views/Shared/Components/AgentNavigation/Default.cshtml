@model IEnumerable<string>

<div class="d-grid gap-2">

    <a class="btn btn-outline-secondary" asp-action="AgentList" asp-controller="Agent" asp-route-role="">
        All Agents
    </a>

    @foreach (string role in Model ?? Enumerable.Empty<string>())
    {
        <a class="btn @(role == ViewBag.SelectedRole ? "btn-danger": "btn-outline-secondary")"
            asp-action="AgentList" asp-controller="Agent"
            asp-route-role="@role"
            asp-route-agentPage="1">
            @role
        </a>
    }

</div>